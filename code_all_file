      AND value IS NOT NULL
  )
  SELECT fy, fp, period_end, filed_date, form, val
  FROM raw_norm
  WHERE tag IN ('SalesRevenueNet','Revenues','RevenueFromContractWithCustomerExcludingAssessedTax')
    AND fp IN ('Q1','Q2','Q3')
  ORDER BY period_end, filed_date
