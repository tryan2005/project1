import pandas as pd
from pathlib import Path

BASE  = r"C:\smilefund_project"
MARTS = rf"{BASE}\warehouse\parquet\marts"

# --- Load aligned TTM ---
a = pd.read_parquet(rf"{MARTS}\revenue_ttm_aligned.parquet")

# --- Step 3: Year-over-year growth ---
aa = a.dropna(subset=["calendar_year","qn","TTM_Revenue"]).copy()
aa["calendar_year"] = aa["calendar_year"].astype(int)
aa["qn"] = aa["qn"].astype(int)

# Keep only last TTM of each calendar year
last_ttm = (
    aa.sort_values(["cik","calendar_year","qn"])
      .groupby(["cik","calendar_year"], as_index=False)
      .last()[["cik","calendar_year","TTM_Revenue"]]
      .rename(columns={"TTM_Revenue":"TTM"})
)

# Prior-year lookup
prior = last_ttm.copy()
prior["calendar_year"] = prior["calendar_year"] + 1
prior = prior.rename(columns={"TTM":"TTM_prior"})

yoy = last_ttm.merge(prior, on=["cik","calendar_year"], how="left")
yoy["YoY_Growth"] = (yoy["TTM"] - yoy["TTM_prior"]) / yoy["TTM_prior"]
yoy.loc[yoy["TTM_prior"].isna() | (yoy["TTM_prior"] == 0), "YoY_Growth"] = pd.NA

out_yoy = rf"{MARTS}\revenue_yoy_true.parquet"
yoy.to_parquet(out_yoy)
print(f"✅ wrote {out_yoy} | rows: {len(yoy):,}")

# --- Step 4: Mapped view with tickers ---
dim  = pd.read_parquet(rf"{MARTS}\dim_security.parquet")

# latest TTM per CIK
last_ttm_overall = (
    a.sort_values(["cik","calendar_year","qn"])
     .groupby("cik", as_index=False)
     .last()[["cik","calendar_year","calendar_quarter","TTM_Revenue"]]
     .rename(columns={
         "calendar_year":"ttm_cal_year",
         "calendar_quarter":"ttm_cal_quarter",
         "TTM_Revenue":"ttm_revenue"
     })
)

vw = (
    dim.merge(last_ttm_overall, on="cik", how="left")
       .merge(yoy[["cik","calendar_year","YoY_Growth"]]
              .rename(columns={"calendar_year":"ttm_cal_year"}), 
              on=["cik","ttm_cal_year"], how="left")
)

out_vw = rf"{MARTS}\vw_mapped_with_ttm.parquet"
vw.to_parquet(out_vw)
print(f"✅ wrote {out_vw} | rows: {len(vw):,}")
print("Null tickers:", vw['ticker'].isna().sum())
print("Null ttm_revenue:", vw['ttm_revenue'].isna().sum())
print("Null YoY_Growth:", vw['YoY_Growth'].isna().sum())

