import os
import pandas as pd
from pathlib import Path

BASE  = r"C:\smilefund_project"
MARTS = rf"{BASE}\warehouse\parquet\marts"
MAP_XLSX = rf"{BASE}\data\sec\ticker_map.xlsx"
MAP_CSV  = rf"{BASE}\data\sec\ticker_map.csv"

dim = pd.read_parquet(rf"{MARTS}\dim_security.parquet")
ttm = pd.read_parquet(rf"{MARTS}\revenue_ttm_aligned.parquet")

# load your map (xlsx or csv)
if os.path.exists(MAP_XLSX):
    tick = pd.read_excel(MAP_XLSX)
elif os.path.exists(MAP_CSV):
    tick = pd.read_csv(MAP_CSV)
else:
    raise FileNotFoundError("ticker_map.xlsx or ticker_map.csv not found in data\\sec")

# normalize columns/values
cols_lower = {c.lower(): c for c in tick.columns}
cik_col    = cols_lower.get("cik") or cols_lower.get("_cik")
ticker_col = cols_lower.get("ticker")
tick = tick.rename(columns={cik_col:"cik", ticker_col:"ticker"})
tick["cik"] = pd.to_numeric(tick["cik"], errors="coerce").astype("Int64")
tick["ticker"] = (tick["ticker"].astype(str).str.upper().str.strip()
                  .str.replace("-", ".", regex=False))
tick = tick.dropna(subset=["cik"]).drop_duplicates(subset=["cik"])

# restrict to mapped CIKs only
dim_mapped = dim.merge(tick, on="cik", how="inner", suffixes=("_dim",""))

# latest TTM per cik
last_ttm = (ttm.sort_values(["cik","calendar_year","qn"])
              .groupby("cik", as_index=False)
              .last()[["cik","calendar_year","calendar_quarter","TTM_Revenue"]])

vw = (dim_mapped.merge(last_ttm, on="cik", how="left")
        .rename(columns={
            "calendar_year":"ttm_cal_year",
            "calendar_quarter":"ttm_cal_quarter",
            "TTM_Revenue":"ttm_revenue"
        }))

outp = rf"{MARTS}\vw_mapped_with_ttm.parquet"
vw.to_parquet(outp)

print(f"✅ wrote {outp} | rows: {len(vw):,}")
print("Null ticker rows (should be 0):", vw["ticker"].isna().sum())
print("Null ttm_revenue rows:", vw["ttm_revenue"].isna().sum())

# previews
print("\nApple:")
print(vw.query("cik == 320193")[["cik","company_name","ticker","ttm_cal_year","ttm_revenue"]].head())
print("\nTop 5 by TTM:")
print(vw.sort_values("ttm_revenue", ascending=False)[["ticker","company_name","ttm_cal_year","ttm_revenue"]].head(5))
import json, glob

for cik in [1163653, 104169, 320193, 789019, 1318605]:  # NMR, WMT, AAPL, MSFT, AMZN (examples)
    f = glob.glob(fr"C:\smilefund_project\data\sec\company_facts\*{cik}*.json")
    if not f:
