pip install tqdm
import pandas as pd, json, os, glob, numpy as np
from tqdm import tqdm

BASE  = r"C:\smilefund_project"
DATA  = rf"{BASE}\data\sec\company_facts"
MARTS = rf"{BASE}\warehouse\parquet\marts"
os.makedirs(MARTS, exist_ok=True)

TAGS_NETINCOME = [
    "NetIncomeLoss",
    "ProfitLoss",
    "NetIncomeLossAvailableToCommonStockholdersBasic",
    "NetIncomeLossAvailableToCommonStockholdersDiluted"
]

rows = []
files = glob.glob(rf"{DATA}\*.json")
for i, f in enumerate(files, 1):
    try:
        data = json.load(open(f))
        cik = int(data.get("cik", 0))
        name = data.get("entityName", "")
        facts = data.get("facts", {}).get("us-gaap", {})
        vals = None
        for tag in TAGS_NETINCOME:
            if tag in facts:
                vals = facts[tag]["units"].get("USD")
                if vals: break
        if not vals: continue
        for v in vals:
            if v.get("fp") not in ["Q1","Q2","Q3","FY"]: continue
            if v.get("form") not in ["10-Q","10-K"]: continue
            if v.get("val") is None: continue
            start, end = v.get("start"), v.get("end")
            if not start or not end: continue
            dur = (pd.Timestamp(end) - pd.Timestamp(start)).days
            if dur < 70 or dur > 1000:  # filters only valid quarter/year
                continue
            rows.append({
                "cik": cik,
                "fy": v.get("fy"),
                "fp": v.get("fp"),
                "form": v.get("form"),
                "filed": v.get("filed"),
                "val": v["val"],
                "start": start,
                "end": end,
                "duration_days": dur,
                "tag_used": tag,
                "company_name": name
            })
    except Exception as e:
        print("⚠️", f, str(e))
    if i % 2000 == 0:
        print(f"scanned {i}/{len(files)} company_facts")

df = pd.DataFrame(rows)
df = df.drop_duplicates(subset=["cik","fy","fp","start","end"])
out_q = rf"{MARTS}\netincome_quarters_true.parquet"
df.to_parquet(out_q)
print(f"✅ Wrote {out_q} — rows: {len(df):,}")

# --- Compute rolling TTM (sum last 4 quarters) ---
df["end"] = pd.to_datetime(df["end"])
df = df.sort_values(["cik","end"])
df["TTM_NetIncome"] = df.groupby("cik")["val"].rolling(4, min_periods=4).sum().reset_index(0,drop=True)
df_ttm = df.dropna(subset=["TTM_NetIncome"]).copy()
out_ttm = rf"{MARTS}\netincome_ttm_true.parquet"
df_ttm.to_parquet(out_ttm)
print(f"✅ Wrote {out_ttm} — rows: {len(df_ttm):,}")
# ===================== Net Income — USD-only, correct durations =====================
# Outputs (replaces previous):
#   marts/netincome_quarters_true.parquet
#   marts/netincome_ttm_true.parquet
# ================================================================================

import os, json
from datetime import datetime
from collections import Counter
from pathlib import Path
from typing import Dict, Iterable, List, Tuple
import pandas as pd

# --------------------- CONFIG ---------------------
BASE   = r"C:\smilefund_project"
FACTS  = rf"{BASE}\data\sec\company_facts"    # *.json from SEC
MARTS  = rf"{BASE}\warehouse\parquet\marts"
Path(MARTS).mkdir(parents=True, exist_ok=True)

# Tag priority: pick the first available per period
NI_TAGS: List[str] = [
    "NetIncomeLoss",
    "ProfitLoss",
    "NetIncomeLossAvailableToCommonStockholdersBasic",
    "NetIncomeLossAvailableToCommonStockholdersDiluted"
]

# Accept only USD-denominated units (strict) and scale properly
USD_UNITS_MULT: Dict[str, int] = {
    "USD": 1,
    "USD$": 1,
    "USDthousands": 1_000,
    "USDThousands": 1_000,
    "USDm": 1_000_000,
    "USDmillions": 1_000_000,
    "USDMillions": 1_000_000,
}

# duration windows (days)
Q_MIN, Q_MAX = 70, 110          # quarter ≈ 90 days
FY_MIN, FY_MAX = 330, 380       # fiscal year ≈ 365 days

def parse_dt(s: str):
    try: return datetime.strptime(s, "%Y-%m-%d")
    except: return None

def duration_days(start: str, end: str) -> int | None:
    s, e = parse_dt(start), parse_dt(end)
    if not s or not e: return None
    return (e - s).days + 1

def iter_netincome_usd_quarters(cik: int, data: dict):
    """Yield only Q1–Q3 (true 10-Q quarter length) and FY (true 10-K year) facts."""
    facts = data.get("facts", {}).get("us-gaap", {})
    for tag in NI_TAGS:
        blk = facts.get(tag)
        if not blk: 
            continue
        for unit_name, arr in blk.get("units", {}).items():
            if unit_name not in USD_UNITS_MULT:
                continue
            mult = USD_UNITS_MULT[unit_name]
            for rec in arr:
                fp, form, fy, val = rec.get("fp"), rec.get("form"), rec.get("fy"), rec.get("val")
                start, end = rec.get("start"), rec.get("end")
                if val is None or fy is None or fp is None or form is None or not start or not end:
                    continue
                d = duration_days(start, end)
                if fp in ("Q1","Q2","Q3"):
                    if not (form == "10-Q" and d and Q_MIN <= d <= Q_MAX): 
                        continue
                elif fp == "FY":
                    if not (form == "10-K" and d and FY_MIN <= d <= FY_MAX):
                        continue
                else:
                    # ignore any Q4 quarter rows; we will compute Q4 via 10K delta
                    continue
                try:
                    value = float(val) * mult
                except:
                    continue
                yield {
                    "cik": int(data.get("cik", 0)),
                    "tag": tag,
                    "unit": unit_name,
                    "fy": int(fy),
                    "fp": str(fp),
                    "form": str(form),
                    "filed": str(rec.get("filed") or ""),
                    "start": start, "end": end, "days": d,
                    "NetIncome": value,
                }

def choose_best_per_period(df: pd.DataFrame) -> pd.DataFrame:
    """Choose one value per (cik, fy, fp) using tag priority then latest filed date."""
    tag_rank = {t: i for i, t in enumerate(NI_TAGS)}
    out = df.copy()
    out["tag_rank"] = out["tag"].map(tag_rank).fillna(9999)
    out["filed_dt"] = pd.to_datetime(out["filed"], errors="coerce")
    out = out.sort_values(
        ["cik","fy","fp","tag_rank","filed_dt"],
        ascending=[True, True, True, True, True]
    )
    best = out.drop_duplicates(subset=["cik","fy","fp"], keep="first")
    return best[["cik","fy","fp","NetIncome","tag","unit","form","filed","start","end","days"]]

def build_quarters_and_delta(best: pd.DataFrame) -> pd.DataFrame:
    """Keep Q1..Q3; compute 10K delta = FY - (Q1+Q2+Q3); assemble Q1..Q3 + delta."""
    qtrs = best[best["fp"].isin(["Q1","Q2","Q3"])].copy()
    ann  = best[(best["fp"] == "FY") & (best["form"] == "10-K")].copy()

    q123 = (qtrs.groupby(["cik","fy"], as_index=False)["NetIncome"].sum()
                 .rename(columns={"NetIncome":"Q123"}))
    merged = ann.merge(q123, on=["cik","fy"], how="inner", validate="one_to_one")
    merged["delta"] = merged["NetIncome"] - merged["Q123"]

    delta_rows = merged[["cik","fy","delta"]].copy()
    delta_rows["fp"] = "10K delta"
    delta_rows.rename(columns={"delta":"NetIncome"}, inplace=True)

    out = pd.concat([
        qtrs[["cik","fy","fp","NetIncome"]],
        delta_rows[["cik","fy","fp","NetIncome"]]
    ], ignore_index=True)

    fp_to_qn = {"Q1":1,"Q2":2,"Q3":3,"10K delta":4}
    out["qn"] = out["fp"].map(fp_to_qn).astype("Int64")
    return out.sort_values(["cik","fy","qn"]).reset_index(drop=True)

# --------------------- MAIN (this step only writes quarters & TTM) ---------------------
files = list(Path(FACTS).glob("*.json"))
rows = []
for i, p in enumerate(files, 1):
    try:
        data = json.load(open(p, "r"))
        for rec in iter_netincome_usd_quarters(int(data.get("cik", 0)), data):
            rows.append(rec)
    except Exception:
        pass
    if i % 2000 == 0:
        print(f"scanned {i}/{len(files)} company_facts (USD-quarterly/FY only)")

raw = pd.DataFrame(rows)
if raw.empty:
    raise RuntimeError("No eligible Net Income facts after duration filtering.")

best = choose_best_per_period(raw)
ni_q = build_quarters_and_delta(best)

# Optional: flip negative deltas if clearly sign-flipped (rare for NI; keep as-is for audit)
# (We skip flipping by default for Net Income; negative Q4 is plausible.)

# Save quarters mart
out_q = rf"{MARTS}\netincome_quarters_true.parquet"
ni_q.to_parquet(out_q)
print(f"✅ Wrote {out_q} — rows: {len(ni_q):,}")

# Build TTM from true quarters (Q1..Q3 + 10K delta = 4 quarters)
q = ni_q.sort_values(["cik","fy","qn"]).copy()
q["TTM_NetIncome"] = (
    q.groupby("cik")["NetIncome"]
     .rolling(4, min_periods=4)
     .sum()
     .reset_index(level=0, drop=True)
)
ni_ttm = q.dropna(subset=["TTM_NetIncome"]).copy()

out_ttm = rf"{MARTS}\netincome_ttm_true.parquet"
ni_ttm.to_parquet(out_ttm)
print(f"✅ Wrote {out_ttm} — rows: {len(ni_ttm):,}")
# ===================== Net Income — Align → YoY → Mapped View =====================
import os, json
from collections import Counter
from pathlib import Path
import pandas as pd

BASE   = r"C:\smilefund_project"
FACTS  = rf"{BASE}\data\sec\company_facts"
MARTS  = rf"{BASE}\warehouse\parquet\marts"

# ---- 0) Load TTM (from your previous step) ----
ni_ttm = pd.read_parquet(rf"{MARTS}\netincome_ttm_true.parquet")

# ---- 1) Get fiscal year-end months (reuse if available; else infer) ----
dim_fye_path = rf"{MARTS}\dim_fiscal_year_end.parquet"
if os.path.exists(dim_fye_path):
    dim_fye = pd.read_parquet(dim_fye_path)
else:
    # Fallback inference (use revenue tags; FY 'end' dates are reliable there)
    REV_TAGS = [
        "SalesRevenueNet",
        "Revenues",
        "RevenueFromContractWithCustomerExcludingAssessedTax",
        "SalesRevenueGoodsNet",
        "SalesRevenueServicesNet",
    ]
    def month_from_yyyy_mm_dd(s):
        try:
            return int(s[5:7]) if s and len(s) >= 7 else None
        except:
            return None

    def infer_fye_month(path: Path):
        data = json.load(open(path, "r"))
        cik = int(data.get("cik", 0))
        months = []
        facts = data.get("facts", {}).get("us-gaap", {})
        for tag in REV_TAGS:
            blk = facts.get(tag)
            if not blk: continue
            for _, arr in blk.get("units", {}).items():
                for rec in arr:
                    if rec.get("fp") == "FY":
                        m = month_from_yyyy_mm_dd(rec.get("end"))
                        if m: months.append(m)
        if not months: 
            return cik, None
        c = Counter(months)
        return cik, max(c.items(), key=lambda kv: (kv[1], kv[0]))[0]

    files = list(Path(FACTS).glob("*.json"))
    rows = []
    for i, p in enumerate(files, 1):
        try: rows.append(infer_fye_month(p))
        except: pass
    dim_fye = pd.DataFrame(rows, columns=["cik","fye_month"]).drop_duplicates("cik")
    dim_fye["fiscal_year_start_month"] = ((dim_fye["fye_month"].fillna(12).astype("Int64") % 12) + 1).astype("Int64")
    dim_fye.to_parquet(dim_fye_path)

# ---- 2) Align TTM to calendar year/quarter ----
def quarter_end_month_from_qn(qn, fye):
    return ((int(fye) + {4:0,3:-3,2:-6,1:-9}[int(qn)] - 1) % 12) + 1
def cal_quarter(m): 
    return (int(m)-1)//3 + 1
def cal_year(fy, end_m, fye_m):
    return int(fy) if int(end_m) <= int(fye_m) else int(fy) - 1

a = ni_ttm.merge(dim_fye, on="cik", how="left")

mask = a["fye_month"].notna() & a["qn"].notna()
end_month, cal_years, cal_quarters = [], [], []
for fy, qn, fye in a.loc[mask, ["fy","qn","fye_month"]].itertuples(index=False):
    em = quarter_end_month_from_qn(qn, fye)
    end_month.append(em)
    cal_years.append(cal_year(fy, em, fye))
    cal_quarters.append(cal_quarter(em))
idx = a.index[mask]
a.loc[idx, "quarter_end_month"] = end_month
a.loc[idx, "calendar_year"]     = cal_years
a.loc[idx, "calendar_quarter"]  = cal_quarters

out_align = rf"{MARTS}\netincome_ttm_aligned.parquet"
a.to_parquet(out_align)
print(f"✅ Wrote {out_align} — rows: {len(a):,}")

# ---- 3) YoY (use last TTM of each calendar year) ----
aa = a.dropna(subset=["calendar_year","qn","TTM_NetIncome"]).copy()
aa["calendar_year"] = aa["calendar_year"].astype(int)
aa["qn"] = aa["qn"].astype(int)

last_ttm = (
    aa.sort_values(["cik","calendar_year","qn"])
      .groupby(["cik","calendar_year"], as_index=False)
      .last()[["cik","calendar_year","TTM_NetIncome"]]
      .rename(columns={"TTM_NetIncome":"TTM"})
)
prior = last_ttm.copy()
prior["calendar_year"] = prior["calendar_year"] + 1
prior = prior.rename(columns={"TTM":"TTM_prior"})

yoy = last_ttm.merge(prior, on=["cik","calendar_year"], how="left")
yoy["YoY_Growth"] = (yoy["TTM"] - yoy["TTM_prior"]) / yoy["TTM_prior"]
yoy.loc[yoy["TTM_prior"].isna() | (yoy["TTM_prior"] == 0), "YoY_Growth"] = pd.NA

out_yoy = rf"{MARTS}\netincome_yoy_true.parquet"
yoy.to_parquet(out_yoy)
print(f"✅ Wrote {out_yoy} — rows: {len(yoy):,}")

# ---- 4) Mapped view with tickers (latest TTM + YoY) ----
dim  = pd.read_parquet(rf"{MARTS}\dim_security.parquet")

last_overall = (
    a.sort_values(["cik","calendar_year","qn"])
     .groupby("cik", as_index=False)
     .last()[["cik","calendar_year","calendar_quarter","TTM_NetIncome"]]
     .rename(columns={
         "calendar_year":"ttm_cal_year",
         "calendar_quarter":"ttm_cal_quarter",
         "TTM_NetIncome":"ttm_netincome"
     })
)

vw = (
    dim.merge(last_overall, on="cik", how="left")
       .merge(yoy[["cik","calendar_year","YoY_Growth"]]
              .rename(columns={"calendar_year":"ttm_cal_year"}), 
              on=["cik","ttm_cal_year"], how="left")
)

out_vw = rf"{MARTS}\vw_mapped_with_ttm_netincome.parquet"
vw.to_parquet(out_vw)
print(f"✅ Wrote {out_vw} | rows: {len(vw):,}")
print("Null tickers:", vw["ticker"].isna().sum())
print("Null ttm_netincome:", vw["ttm_netincome"].isna().sum())
print("Null YoY_Growth:", vw["YoY_Growth"].isna().sum())

# ---- 5) Quick sanity checks (Walmart & Apple) ----
def show(cik):
    r = vw.loc[vw["cik"]==cik, ["ticker","company_name","ttm_cal_year","ttm_netincome","YoY_Growth"]]
    return r.tail(3)

print("\nWalmart (104169):")
print(show(104169))

print("\nApple (320193):")
print(show(320193))
import pandas as pd

MARTS = r"C:\smilefund_project\warehouse\parquet\marts"
CIK_WMT = 104169

# 1) What our pipeline used (CALENDAR YoY from TTM)
a = pd.read_parquet(rf"{MARTS}\netincome_ttm_aligned.parquet")  # <-- use revenue_ttm_aligned.parquet if checking revenue
w = a[a.cik == CIK_WMT].sort_values(["calendar_year","qn"])
cal_last = (w.dropna(subset=["calendar_year","qn","TTM_NetIncome"])  # <-- TTM_Revenue if revenue
             .assign(calendar_year=lambda d: d["calendar_year"].astype(int),
                     qn=lambda d: d["qn"].astype(int))
             .sort_values(["calendar_year","qn"])
             .groupby(["cik","calendar_year"], as_index=False)
             .last()[["cik","calendar_year","TTM_NetIncome"]])      # <-- TTM_Revenue if revenue
cal_last = cal_last.rename(columns={"TTM_NetIncome":"TTM"})          # <-- TTM_Revenue -> TTM

cal_prior = cal_last.rename(columns={"TTM":"TTM_prior","calendar_year":"calendar_year_prior"})
cal_prior["calendar_year"] = cal_prior["calendar_year_prior"] + 1
cal_yoy = (cal_last.merge(cal_prior[["cik","calendar_year","TTM_prior"]],
                          on=["cik","calendar_year"], how="left")
                   .assign(YoY_Calendar=(cal_last["TTM"]-cal_prior["TTM_prior"])/cal_prior["TTM_prior"]))

print("=== Calendar TTM YoY we used ===")
print(cal_yoy.tail(5))

# 2) Fiscal-year YoY (sum Q1–Q3 + 10K delta = FY), then compare FY(t) vs FY(t-1)
q = pd.read_parquet(rf"{MARTS}\netincome_quarters_true.parquet")     # <-- revenue_quarters_true.parquet if revenue
qw = q[q.cik == CIK_WMT]
fy_tot = (qw.pivot_table(index=["cik","fy"], columns="fp", values="NetIncome", aggfunc="sum")  # <-- Revenue if revenue
            .fillna(0))
# FY total = Q1+Q2+Q3 + 10K delta  (this reconstructs the 10-K FY value)
fy_tot["FY_total"] = fy_tot.get("Q1",0)+fy_tot.get("Q2",0)+fy_tot.get("Q3",0)+fy_tot.get("10K delta",0)
fy_tot = fy_tot.reset_index()[["cik","fy","FY_total"]].sort_values("fy")

fy_prior = fy_tot.rename(columns={"FY_total":"FY_prior","fy":"fy_prior"})
fy_prior["fy"] = fy_prior["fy_prior"] + 1
fy_yoy = (fy_tot.merge(fy_prior[["cik","fy","FY_prior"]], on=["cik","fy"], how="left")
                .assign(YoY_Fiscal=(fy_tot["FY_total"]-fy_prior["FY_prior"])/fy_prior["FY_prior"]))

print("\n=== Fiscal 10-K YoY (reconstructed) ===")
print(fy_yoy.tail(5))

# 3) Convenience: show the specific year you're questioning
target_years = cal_yoy["calendar_year"].dropna().astype(int).sort_values().tail(2).tolist()
print("\nComparing the most recent two calendar years used for YoY:")
print(cal_yoy[cal_yoy["calendar_year"].isin(target_years)])
