import os
import pandas as pd
from pathlib import Path

BASE  = r"C:\smilefund_project"
MARTS = rf"{BASE}\warehouse\parquet\marts"
LOGS  = rf"{MARTS}\_logs"
Path(LOGS).mkdir(parents=True, exist_ok=True)

dim_path   = rf"{MARTS}\dim_security.parquet"
map_xlsx   = rf"{BASE}\data\sec\ticker_map.xlsx"   # your file
map_csv    = rf"{BASE}\data\sec\ticker_map.csv"    # fallback if you saved as CSV

# --- load dim_security ---
dim = pd.read_parquet(dim_path)

# --- load your ticker map (xlsx or csv) ---
if os.path.exists(map_xlsx):
    tick = pd.read_excel(map_xlsx)
elif os.path.exists(map_csv):
    tick = pd.read_csv(map_csv)
else:
    raise FileNotFoundError("Couldn't find ticker_map.xlsx or ticker_map.csv in data\\sec")

# --- normalize columns (accept 'cik' or '_cik') ---
cols_lower = {c.lower(): c for c in tick.columns}
cik_col     = cols_lower.get("cik") or cols_lower.get("_cik")
ticker_col  = cols_lower.get("ticker")
if cik_col is None or ticker_col is None:
    raise RuntimeError(f"Expected columns 'ticker' and 'cik' (or '_cik'). Got: {list(tick.columns)}")

tick = tick.rename(columns={cik_col: "cik", ticker_col: "ticker"})

# --- clean values ---
tick["cik"] = pd.to_numeric(tick["cik"], errors="coerce").astype("Int64")
tick = tick.dropna(subset=["cik"])
tick["ticker"] = (tick["ticker"].astype(str)
                              .str.upper()
                              .str.strip()
                              .str.replace("-", ".", regex=False))   # e.g., BRK-B -> BRK.B

# --- log duplicate CIKs in your map (if any) and keep the first occurrence ---
dup_mask = tick.duplicated(subset=["cik"], keep=False)
if dup_mask.any():
    tick.loc[dup_mask].to_csv(rf"{LOGS}\ticker_map_duplicates.csv", index=False)
    print(f"⚠️ Duplicates found in your ticker map; logged to {LOGS}\\ticker_map_duplicates.csv")
tick = tick.drop_duplicates(subset=["cik"], keep="first")

# --- merge into dim_security (your map takes precedence) ---
dim_upd = dim.merge(tick[["cik","ticker"]], on="cik", how="left", suffixes=("", "_map"))
dim_upd["ticker"] = dim_upd["ticker_map"].combine_first(dim_upd["ticker"])
dim_upd = dim_upd.drop(columns=["ticker_map"])

# --- save ---
dim_upd.to_parquet(dim_path)
print(f"✅ Updated {dim_path} with {len(tick):,} mapped tickers.")

# quick spot-checks (AAPL, MSFT if present)
for c in [320193, 789019]:
    row = dim_upd.loc[dim_upd["cik"] == c, ["cik","company_name","ticker"]]
    if not row.empty:
        print(row)
