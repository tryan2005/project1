aa = a.dropna(subset=["calendar_year","qn","TTM_NetIncome"]).copy()
aa["calendar_year"] = aa["calendar_year"].astype(int)
aa["qn"] = aa["qn"].astype(int)

last_ttm = (
    aa.sort_values(["cik","calendar_year","qn"])
      .groupby(["cik","calendar_year"], as_index=False)
      .last()[["cik","calendar_year","TTM_NetIncome"]]
      .rename(columns={"TTM_NetIncome":"TTM"})
)
prior = last_ttm.copy()
prior["calendar_year"] = prior["calendar_year"] + 1
prior = prior.rename(columns={"TTM":"TTM_prior"})

yoy = last_ttm.merge(prior, on=["cik","calendar_year"], how="left")
yoy["YoY_Growth"] = (
    (yoy["TTM"].astype(float) - yoy["TTM_prior"].astype(float))
    / yoy["TTM_prior"].astype(float)
)
yoy.loc[yoy["TTM_prior"].isna() | (yoy["TTM_prior"] == 0), "YoY_Growth"] = pd.NA

out_yoy = rf"{MARTS}\netincome_yoy_true.parquet"
